{
  "name": "template-repo",
  "version": "1.1.5",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -rf dist/",
    "install-dev-dependencies": "npm install --save-dev webpack webpack-cli webpack-dev-server webpack-merge style-loader css-loader html-loader html-webpack-plugin",
    "deploy": "git checkout gh-pages && git merge main --no-edit && git add dist -f && git commit -m 'Deployment commit' && git subtree push --prefix dist origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aleskoss/template-repo.git"
  },
  "author": "Ale≈° Molin <molin.ales@icloud.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aleskoss/template-repo/issues"
  },
  "homepage": "https://github.com/Aleskoss/template-repo#readme",
  "devDependencies": {
    "css-loader": "^7.1.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "webpack": "^5.100.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  }
}
